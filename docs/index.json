&& git commit --amend --no-edit\"\n  },\n  \"jest\": {\n    \"modulePaths\": [\n      \"src\"\n    ],\n    \"moduleFileExtensions\": [\n      \"js\"\n    ],\n    \"collectCoverageFrom\": [\n      \"**/src/**/*.{js}\"\n    ],\n    \"moduleNameMapper\": {\n      \"\\\\.(jpeg|jpg|png|gif|eot|webp|svg|ttf|woff2|mp3|webm|wav|mp3|m4a|aac|oga)$\": \"<rootDir>/__mocks__/fileMock.js\",\n      \"\\\\.(css|scss)$\": \"<rootDir>/__mocks__/styleMock.js\"\n    },\n    \"coverageReporters\": [\n      \"json\",\n      \"lcov\",\n      \"text\"\n    ]\n  },\n  \"standard\": {\n    \"parser\": \"babel-eslint\",\n    \"plugins\": \"flowtype\",\n    \"ignore\": [\n      \"**/dist/\",\n      \"**/flow-typed\",\n      \"**/docs\"\n    ],\n    \"globals\": [\n      \"jest\",\n      \"describe\",\n      \"it\",\n      \"expect\",\n      \"HTMLElement\",\n      \"HTMLInputElement\",\n      \"Document\"\n    ]\n  },\n  \"license\": \"MIT\",\n  \"private\": false,\n  \"author\": {\n    \"name\": \"Theo Salzmann\",\n    \"email\": \"dev@on3iro.de\"\n  },\n  \"contributors\": [],\n  \"homepage\": \"-\",\n  \"dependencies\": {\n    \"babel-polyfill\": \"6.26.0\"\n  },\n  \"engines\": {},\n  \"devDependencies\": {\n    \"babel-cli\": \"^6.26.0\",\n    \"babel-core\": \"6.26.0\",\n    \"babel-eslint\": \"8.0.3\",\n    \"babel-jest\": \"22.0.4\",\n    \"babel-plugin-external-helpers\": \"6.22.0\",\n    \"babel-preset-env\": \"1.6.1\",\n    \"babel-preset-flow\": \"6.23.0\",\n    \"babel-preset-stage-3\": \"6.24.1\",\n    \"coveralls\": \"3.0.0\",\n    \"cross-env\": \"5.1.3\",\n    \"esdoc\": \"^1.0.4\",\n    \"esdoc-standard-plugin\": \"^1.0.0\",\n    \"eslint-loader\": \"1.9.0\",\n    \"eslint-plugin-flowtype\": \"2.41.0\",\n    \"flow-bin\": \"^0.63.1\",\n    \"flow-copy-source\": \"^1.2.2\",\n    \"glow\": \"^1.2.2\",\n    \"jest\": \"22.0.5\",\n    \"jsdoc\": \"3.5.5\",\n    \"jsdoc-babel\": \"0.3.0\",\n    \"rimraf\": \"2.6.2\",\n    \"rollup\": \"0.53.4\",\n    \"rollup-plugin-babel\": \"3.0