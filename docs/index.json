    "content": "// @flow\n\nimport {\n  validate,\n  validateValue,\n  makeValidate,\n  makeValidationWithBlurBindings\n} from './validation'\n\nimport {\n  isEmail,\n  isEmpty,\n  isNoneEmptyString,\n  isNumberString,\n  isString\n} from './validators'\n\nconst vally = {\n  isEmail,\n  isEmpty,\n  isNoneEmptyString,\n  isNumberString,\n  isString,\n  makeValidate,\n  makeValidationWithBlurBindings,\n  validate,\n  validateValue\n}\n\nexport {\n  vally,\n  isEmail,\n  isEmpty,\n  isNoneEmptyString,\n  isNumberString,\n  isString,\n  makeValidate,\n  makeValidationWithBlurBindings,\n  validate,\n  validateValue\n}\n",
